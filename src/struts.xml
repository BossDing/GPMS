<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true" />

	<package name="gpms" namespace="/" extends="struts-default">
		<default-action-ref name="index" />
		<action name="index">
			<result>
				jsp/index.jsp
			</result>
		</action>
		<action name="login">
			<result>
				jsp/login.jsp
			</result>
		</action>
		<action name="changePasswd">
			<result>
				jsp/changePasswd.jsp
			</result>
		</action>
		<action name="student">
			<result>
				jsp/student/student.jsp
			</result>
		</action>
		<action name="teacher">
			<result>
				jsp/teacher/teacher.jsp
			</result>
		</action>
		<action name="teacherSubject">
			<result>
				jsp/teacher/teacherSubject.jsp
			</result>
		</action>
		<action name="template">
			<result>
				jsp/template/page.jsp
			</result>
		</action>
		<action name="logout" class="com.find1x.gpms.action.LogoutAction">
			<result type="redirect">
				/
			</result>
		</action>
		<action name="Login" class="com.find1x.gpms.action.LoginAction">
			<result type="redirect">
				/
			</result>
			<result name="error" type="redirect">
				/?status=error
			</result>
		</action>
		<action name="ChangePasswd" class="com.find1x.gpms.action.ChangePasswdAction">
			<result>
				jsp/result.jsp
			</result>
			<result name="error" type="redirect">
				jsp/changePasswd?status=error
			</result>
		</action>
		<action name="UploadIssue" class="com.find1x.gpms.action.UploadIssueAction">
			<result>
				jsp/teacher/teacherSubjectManage.jsp
			</result>
		</action>
		<action name="ChangeIssue" class="com.find1x.gpms.action.ChangeIssueAction">
			<result>
				jsp/teacher/teacherSubjectManage.jsp
			</result>
		</action>
		<action name="teacherSubjectManage"
			class="com.find1x.gpms.action.teacherSubjectManageAction">
			<result>
				jsp/teacher/teacherSubjectManage.jsp
			</result>
		</action>
		<action name="teacherChangeSubject"
			class="com.find1x.gpms.action.teacherChangeSubjectAction">
			<result>
				jsp/teacher/teacherChangeSubject.jsp
			</result>
		</action>
		<action name="teacherInfo" class="com.find1x.gpms.action.teacherInfoAction">
			<result>
				jsp/teacher/teacherInfo.jsp
			</result>
		</action>
		<action name="teacherStudent" class="com.find1x.gpms.action.teacherStudentAction">
			<result>
				jsp/teacher/teacherStudent.jsp
			</result>
		</action>
		<action name="teacherStudentGrade" class="com.find1x.gpms.action.teacherStudentGradeAction">
			<result>
				jsp/teacher/teacherStudentGrade.jsp
			</result>
		</action>
		<action name="teacherStudentDetail"
			class="com.find1x.gpms.action.teacherStudentDetailAction">
			<result>
				jsp/teacher/teacherStudentDetail.jsp
			</result>
		</action>

		<action name="teacherUploadFiles" class="com.find1x.gpms.action.teacherUploadFilesAction">
			<result>
				jsp/teacher/teacherUploadFiles.jsp
			</result>
		</action>

		<action name="studentInfo" class="com.find1x.gpms.action.studentInfoAction">
			<result>
				jsp/student/studentInfo.jsp
			</result>
		</action>

		<action name="studentTeacher" class="com.find1x.gpms.action.studentTeacherAction">
			<result>
				jsp/student/studentTeacher.jsp
			</result>

		</action>

		<action name="studentGrade" class="com.find1x.gpms.action.studentGradeAction">
			<result>
				jsp/student/studentGrade.jsp
			</result>
		</action>
	</package>
</struts>
